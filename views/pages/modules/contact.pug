div.contact 
    div.container 
        div.contact-content 
            div.contact-map-form
                div.contact__map
                    iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2924.0781498599813!2d74.56783416578286!3d42.871194629155916!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x389ec83f60e88e83%3A0x54e6e83b1a0c8b96!2z0JvQtdC90LjQvdGB0LrQuNC5INGA0LDQudC-0L0!5e0!3m2!1sru!2skg!4v1661518499379!5m2!1sru!2skg" width="100%" height="100%" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade")
                div.contact__form 
                    input(placeholder="Your Name")
                    input(placeholder="Your Email")
                    input(placeholder="Subject")
                    textarea(name="", cols="30", rows="10") 
                    button.form__button.button Send Message
            div.contact__information
                h3 Contact Informaton
                div.contact__information__list
                    div
                        p Address: #{" "}
                            a 198 West 21th Street, Suite 721 New York NY 10016 
                    div 
                        p 
                            span Phone: #{" "}
                            a(href="tel://1234567920") + 1235 2355 98
                    div 
                        p 
                            span Email: #{" "}
                            a(href="mailto:info@yoursite.com") info@yoursite.com
                    div
                        p 
                            span Website: #{" "}
                            a(href="/") yoursite.com