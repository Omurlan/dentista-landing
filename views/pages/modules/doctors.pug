block pageVariables 
    -
        const doctors = [
            { image: "./images/doctor-1.webp", delay: 0.1, name: "Dr. Lloyd Wilson", title: "Dentist", desc: "I am an ambitious workaholic, but apart from that, pretty simple person." },
            { image: "./images/doctor-2.webp", delay: 0.15, name: "Dr. Rachel Parker", title: "Dentist", desc: "I am an ambitious workaholic, but apart from that, pretty simple person." },
            { image: "./images/doctor-3.webp", delay: 0.2, name: "Dr. Ian Smith", title: "Dentist", desc: "I am an ambitious workaholic, but apart from that, pretty simple person." },
            { image: "./images/doctor-4.webp", delay: 0.25, name: "Dr. Alicia Henderson", title: "Dentist", desc: "I am an ambitious workaholic, but apart from that, pretty simple person." }
        ]
section.doctors 
    div.container
        div.doctors-content
            div.introduction.wow.fadeInUp
                h1.introduction__heading Doctors 
                h3.introduction__subheading Our Qualified Doctors 
                p.introduction__description Separated they live in. A small river named Duden flows by their place and supplies it with the necessary regelialia. It is a paradisematic country
            ul.doctor-list 
                each item, index in doctors 
                    li.doctor-list__item.wow.fadeInUp(data-wow-delay=item.delay + "s")
                        div(style='background-image: url('+ item.image +')').doctor-list__image
                        h3.doctor-list__name #{item.name}
                        h5.doctor-list__title #{item.title}
                        p.doctor-list__description #{item.desc}
            